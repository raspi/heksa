{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Show help",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "args": [
        "--help"
      ]
    },
    {
      "name": "Dump Makefile",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "args": [
        "Makefile"
      ]
    },
    {
      "name": "Dump /dev/urandom",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "args": [
        "-l",
        "1KiB",
        "/dev/urandom"
      ]
    }
  ]
}